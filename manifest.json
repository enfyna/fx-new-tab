{
  "manifest_version": 2,
  "version": "1.9.0",
  "name": "New Tab",
  "description":"A new look for firefox homepage and the newtab page.",
  "icons": {
    "48": "icons/icon.svg",
    "96": "icons/icon.svg",
    "128": "icons/icon.svg"
  },  
  "permissions": [
    "storage",
    "history"
  ],
  "chrome_url_overrides": { 
    "newtab": "index.html" 
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  }
}